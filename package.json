{
  "name": "expo-azure-blob-storage",
  "version": "0.0.1",
  "description": "Easy Azure Blob Storage uploads for Expo and React Native applications",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch ./src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/katungi/expo-azure-blob-storage.git"
  },
  "keywords": [
    "expo",
    "react-native",
    "azure",
    "blob-storage",
    "file-upload",
    "image-upload",
    "azure-storage",
    "react-native-azure",
    "expo-azure"
  ],
  "author": "Katungi Dennis <dankatdennis@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/katungi/expo-azure-blob-storage",
  "bugs": {
    "url": "https://github.com/katungi/expo-azure-blob-storage/issues"
  },
  "sideEffects": false,
  "dependencies": {
    "expo-constants": ">=17.0.0",
    "expo-file-system": ">=15.0.0",
    "expo-image-picker": "^16.1.4"
  },
  "peerDependencies": {
    "expo": ">=49.0.0",
    "react": ">=18.0.0",
    "react-native": ">=0.70.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/node": "^18.0.0",
    "@types/react": "~19.0.10",
    "expo": "~53.0.16",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "tsdown": "^0.16.1",
    "typescript": "~5.8.3"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
