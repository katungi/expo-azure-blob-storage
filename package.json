{
  "name": "expo-azure-blob-storage",
  "version": "0.0.1",
  "description": "Easy Azure Blob Storage uploads for Expo and React Native applications",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/cjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js"
    }
  },
  "scripts": {
    "build": "pnpm run clean && pnpm run build:cjs && pnpm run build:esm",
    "build:cjs": "tsc",
    "build:esm": "tsc -p tsconfig.esm.json",
    "clean": "rm -rf lib",
    "dev": "tsc --watch",
    "prepare": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/katungi/expo-azure-blob-storage.git"
  },
  "keywords": [
    "expo",
    "react-native",
    "azure",
    "blob-storage",
    "file-upload",
    "image-upload",
    "azure-storage",
    "react-native-azure",
    "expo-azure"
  ],
  "author": "Katungi Dennis <dankatdennis@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/katungi/expo-azure-blob-storage",
  "bugs": {
    "url": "https://github.com/katungi/expo-azure-blob-storage/issues"
  },
  "peerDependencies": {
    "expo": ">=49.0.0",
    "react": ">=18.0.0",
    "react-native": ">=0.70.0",
    "expo-constants": ">=17.0.0",
    "expo-file-system": ">=15.0.0",
    "expo-image-picker": "^16.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/node": "^18.0.0",
    "@types/react": "~19.0.10",
    "expo": "~53.0.16",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "typescript": "~5.8.3",
    "expo-constants": ">=17.0.0",
    "expo-file-system": ">=15.0.0",
    "expo-image-picker": "^16.1.4"
  },
  "files": [
    "lib",
    "README.md",
    "package.json"
  ]
}
